{
  "name": "subito-lib",
  "version": "0.1.16",
  "description": "Library for subito micro services",
  "main": "./dist/subito-lib.js",
  "typings": "./dist/subito-lib.d.ts",
  "type": "module",
  "scripts": {
    "bundle": "webpack --config=webpack.config.js",
    "test": "node --experimental-vm-modules node_modules/.bin/jest",
    "build-doc": "api-extractor run --local --verbose",
    "build": "./node_modules/.bin/tsc -p tsconfig.build.json",
    "build-dev": "npm run build && NODE_ENV=development node --inspect lib/index.js",
    "dev": "nodemon --ext ts --exec \"npm run build-dev\"",
    "start": "npm run build && NODE_ENV=production node lib/index.js"
  },
  "author": "Xavius",
  "license": "EUPL-1.2",
  "devDependencies": {
    "@tsconfig/node16": "^1.0.2",
    "@types/jest": "^27.4.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^17.0.12",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "eslint": "^8.7.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.0.0",
    "eslint-plugin-no-secrets": "^0.8.9",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-tsdoc": "^0.2.14",
    "jest": "^27.4.7",
    "lint-staged": "^12.3.2",
    "nodemon": "^2.0.15",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5",
    "webpack": "^5.67.0",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "crypto": "^1.0.1",
    "graphql-request": "^3.7.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1"
  }
}
