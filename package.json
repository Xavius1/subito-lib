{
  "name": "subito-lib",
  "version": "0.14.1",
  "description": "Library for subito micro services",
  "main": "./dist/index.js",
  "typings": "./dist/subito-lib.d.ts",
  "type": "module",
  "scripts": {
    "np": "npm run pre-publish && np",
    "pre-publish": "npm run compile-js && npm run build-types && npm run build-dist",
    "build-dist": "./node_modules/.bin/tsc -p tsconfig.dist.json",
    "build-doc": "./node_modules/.bin/subito-build-doc",
    "build-types": "./node_modules/.bin/subito-build-types",
    "compile-js": "./node_modules/.bin/subito-compile-js",
    "run-tests": "./node_modules/.bin/subito-run-tests",
    "test": "node --experimental-vm-modules node_modules/.bin/jest",
    "build": "./node_modules/.bin/tsc -p tsconfig.build.json",
    "build-dev": "npm run compile-js && NODE_ENV=development node --inspect lib/index.js",
    "dev": "nodemon --ext ts --exec \"npm run build-dev\"",
    "start": "npm run compile-js && NODE_ENV=production node lib/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Xavius1/subito-lib.git"
  },
  "author": "Xavius",
  "license": "EUPL-1.2",
  "bugs": {
    "url": "https://github.com/Xavius1/subito-lib/issues"
  },
  "homepage": "https://github.com/Xavius1/subito-lib#readme",
  "devDependencies": {
    "@tsconfig/node16": "1.0.3",
    "@types/argv": "0.0.5",
    "@types/jest": "28.1.8",
    "@types/jsonwebtoken": "8.5.9",
    "@types/node": "17.0.12",
    "@typescript-eslint/eslint-plugin": "5.41.0",
    "@typescript-eslint/parser": "5.41.0",
    "eslint": "8.26.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-import-resolver-typescript": "3.5.2",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "27.1.3",
    "eslint-plugin-no-secrets": "0.8.9",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-tsdoc": "0.2.17",
    "jest": "28.1.3",
    "lint-staged": "13.0.3",
    "nodemon": "2.0.20",
    "subito-ci": "0.1.37",
    "ts-jest": "28.0.8",
    "ts-node": "10.9.1",
    "typescript": "4.8.4",
    "webpack": "5.74.0",
    "webpack-cli": "4.10.0"
  },
  "dependencies": {
    "@opentelemetry/api": "1.2.0",
    "@opentelemetry/core": "1.6.0",
    "@opentelemetry/exporter-jaeger": "1.6.0",
    "@opentelemetry/exporter-trace-otlp-http": "0.32.0",
    "@opentelemetry/instrumentation": "0.32.0",
    "@opentelemetry/instrumentation-amqplib": "0.31.0",
    "@opentelemetry/instrumentation-express": "0.31.0",
    "@opentelemetry/instrumentation-graphql": "0.30.0",
    "@opentelemetry/instrumentation-http": "0.32.0",
    "@opentelemetry/resources": "1.6.0",
    "@opentelemetry/sdk-trace-base": "1.6.0",
    "@opentelemetry/sdk-trace-node": "1.6.0",
    "argv": "0.0.2",
    "graphql-request": "^5.0.0",
    "jsonwebtoken": "8.5.1",
    "moment-timezone": "0.5.37",
    "moment-with-locales-es6": "1.0.1"
  }
}
